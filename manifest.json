{
  "manifest_version": 3,
  "name": "Raccoonify",
  "version": "0.1.0",
  "description": "Improves boring youtube thumbnails by putting raccoons instead",
  "host_permissions": [
    "*://*.youtube.com/*",
    "*://*.youtu.be/*",
    "https://api.racc.lol/*"
  ],
  "permissions": [
    "webNavigation"
  ],
  "content_scripts":
  [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["src/image.js", "liens.js"],
      "run_at": "document_idle"
    }
  ]
}