{
    "manifest_version": 3,
    "name": "Racoon explorer",
    "version": "1.0",
    "description": "Ajouter Racoon devant les mots dans la partie explorer de YouTube",
    "permissions": [
        "scripting",
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "https://www.youtube.com/*"
    ],
    "background": {
    "service_worker": "background.js"
    },
    "action": {
        "default_title": "modifier le texte"
    }
}