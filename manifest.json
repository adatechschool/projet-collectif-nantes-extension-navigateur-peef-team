{
  "manifest_version": 3,
  "name": "YouTube Redirector",
  "version": "1.0",
  "description": "Redirige les vid√©os YouTube vers api.racc.lol",
  "permissions": [
      "webNavigation"
  ],
  "host_permissions": [
      "*://*.youtube.com/*",
      "*://*.youtu.be/*",
      "https://api.racc.lol/*"
  ],
  "content_scripts": [{
      "matches": [
          "*://*.youtube.com/*",
          "*://*.youtu.be/*"
      ],
      "js": ["liens.js"],
      "run_at": "document_start"
  }]
}